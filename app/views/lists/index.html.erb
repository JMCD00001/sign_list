<h1>Listing lists</h1>

<table>
  <tr>
    <th>Riding</th>
    <th>Street</th>
    <th>Number</th>
    <th>Name</th>
    <th>Occupation</th>
    <th>Phone</th>
    <th>2011</th>
    <th>2008</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lists.each do |list| %>
  <tr>
    <td><%= list.riding %></td>
    <td><%= list.street %></td>
    <td><%= list.number %></td>
    <td><%= list.name %></td>
    <td><%= list.occupation %></td>
    <td><%= list.phone %></td>
    <td><%= list.2011 %></td>
    <td><%= list.2008 %></td>
    <td><%= link_to 'Show', list %></td>
    <td><%= link_to 'Edit', edit_list_path(list) %></td>
    <td><%= link_to 'Destroy', list, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New List', new_list_path %>
